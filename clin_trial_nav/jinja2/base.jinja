    <script>
      $(function() {
        $('#table').bootstrapTable()
      })
    </script>

    <script>
      var table = $('#table')
      
      $(function () {
        table.bootstrapTable({
            exportDataType: $(this).val(),
            exportTypes: ['json', 'xml', 'csv', 'txt', 'sql', 'excel', 'pdf'],
            })
        })
    </script>
   

